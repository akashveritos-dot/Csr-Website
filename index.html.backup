    <script>
        // Enhanced dropdown functionality with stunning interactions
        document.addEventListener('DOMContentLoaded', function() {
            const dropdowns = document.querySelectorAll('.dropdown');
            const navLinks = document.querySelectorAll('.nav-link');
            const dropdownLinks = document.querySelectorAll('.dropdown-links a');
            const knowMoreButtons = document.querySelectorAll('.know-more');
            
            // Add smooth reveal animation for dropdowns
            dropdowns.forEach(dropdown => {
                const dropdownContent = dropdown.querySelector('.dropdown-content');
                let timeout;
                let isOpen = false;
                let isHoveringDropdown = false;
                let isHoveringContent = false;
                
                dropdown.addEventListener('mouseenter', function() {
                    isHoveringDropdown = true;
                    clearTimeout(timeout);
                    if (!isOpen) {
                        dropdownContent.style.display = 'block';
                        dropdownContent.style.opacity = '0';
                        dropdownContent.style.transform = 'translateX(-50%) translateY(-10px) scale(0.95)';
                        
                        setTimeout(() => {
                            dropdownContent.style.opacity = '1';
                            dropdownContent.style.transform = 'translateX(-50%) translateY(0) scale(1)';
                            dropdownContent.style.visibility = 'visible';
                            isOpen = true;
                        }, 10);
                    }
                });
                
                dropdown.addEventListener('mouseleave', function() {
                    isHoveringDropdown = false;
                    checkCloseDropdown();
                });
                
                dropdownContent.addEventListener('mouseenter', function() {
                    isHoveringContent = true;
                    clearTimeout(timeout);
                });
                
                dropdownContent.addEventListener('mouseleave', function() {
                    isHoveringContent = false;
                    checkCloseDropdown();
                });
                
                function checkCloseDropdown() {
                    timeout = setTimeout(() => {
                        if (!isHoveringDropdown && !isHoveringContent) {
                            dropdownContent.style.opacity = '0';
                            dropdownContent.style.transform = 'translateX(-50%) translateY(-10px) scale(0.95)';
                            dropdownContent.style.visibility = 'hidden';
                            
                            setTimeout(() => {
                                dropdownContent.style.display = 'none';
                                isOpen = false;
                            }, 400);
                        }
                    }, 200); // Increased timeout for better UX
                }
                
                // Add click functionality for mobile
                dropdown.addEventListener('click', function(e) {
                    if (window.innerWidth <= 768) {
                        e.preventDefault();
                        if (!isOpen) {
                            // Close other dropdowns
                            dropdowns.forEach(other => {
                                if (other !== dropdown) {
                                    const otherContent = other.querySelector('.dropdown-content');
                                    otherContent.style.display = 'none';
                                    otherContent.style.opacity = '0';
                                    otherContent.style.visibility = 'hidden';
                                }
                            });
                            
                            dropdownContent.style.display = 'block';
                            setTimeout(() => {
                                dropdownContent.style.opacity = '1';
                                dropdownContent.style.visibility = 'visible';
                                dropdownContent.style.transform = 'translateX(-50%) translateY(0) scale(1)';
                            }, 10);
                            isOpen = true;
                        } else {
                            dropdownContent.style.opacity = '0';
                            dropdownContent.style.visibility = 'hidden';
                            setTimeout(() => {
                                dropdownContent.style.display = 'none';
                            }, 400);
                            isOpen = false;
                        }
                    }
                });
            });
            
            // Enhanced ripple effect for navigation links
            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    const ripple = document.createElement('span');
                    const rect = this.getBoundingClientRect();
                    const size = Math.max(rect.width, rect.height);
                    const x = e.clientX - rect.left - size / 2;
                    const y = e.clientY - rect.top - size / 2;
                    
                    ripple.style.width = ripple.style.height = size + 'px';
                    ripple.style.left = x + 'px';
                    ripple.style.top = y + 'px';
                    ripple.classList.add('ripple');
                    
                    this.appendChild(ripple);
                    
                    setTimeout(() => {
                        ripple.remove();
                    }, 600);
                });
            });
            
            // Add click feedback for dropdown links
            dropdownLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    // Create ripple effect
                    const ripple = document.createElement('span');
                    const rect = this.getBoundingClientRect();
                    const size = Math.max(rect.width, rect.height);
                    const x = e.clientX - rect.left - size / 2;
                    const y = e.clientY - rect.top - size / 2;
                    
                    ripple.style.width = ripple.style.height = size + 'px';
                    ripple.style.left = x + 'px';
                    ripple.style.top = y + 'px';
                    ripple.style.position = 'absolute';
                    ripple.style.borderRadius = '50%';
                    ripple.style.background = 'rgba(76, 172, 72, 0.3)';
                    ripple.style.transform = 'scale(0)';
                    ripple.style.animation = 'ripple-animation 0.6s ease-out';
                    ripple.style.pointerEvents = 'none';
                    
                    this.style.position = 'relative';
                    this.appendChild(ripple);
                    
                    setTimeout(() => {
                        ripple.remove();
                    }, 600);
                    
                    // Add visual feedback
                    this.style.transform = 'translateX(5px) rotateY(2deg) scale(0.98)';
                    setTimeout(() => {
                        this.style.transform = '';
                    }, 150);
                });
            });
            
            // Add click feedback for know-more buttons
            knowMoreButtons.forEach(button => {
                button.addEventListener('click', function(e) {
                    e.preventDefault();
                    
                    // Create pulse effect
                    const pulse = document.createElement('span');
                    pulse.style.position = 'absolute';
                    pulse.style.top = '50%';
                    pulse.style.left = '50%';
                    pulse.style.width = '100%';
                    pulse.style.height = '100%';
                    pulse.style.borderRadius = '25px';
                    pulse.style.background = 'rgba(255, 255, 255, 0.4)';
                    pulse.style.transform = 'translate(-50%, -50%) scale(0)';
                    pulse.style.animation = 'pulse-animation 0.6s ease-out';
                    pulse.style.pointerEvents = 'none';
                    
                    this.style.position = 'relative';
                    this.appendChild(pulse);
                    
                    setTimeout(() => {
                        pulse.remove();
                    }, 600);
                    
                    // Add visual feedback
                    this.style.transform = 'translateY(-2px) rotateX(5deg) scale(1.05)';
                    setTimeout(() => {
                        this.style.transform = '';
                    }, 150);
                });
            });
            
            // Enhanced dropdown icon animation
            const dropdownIcons = document.querySelectorAll('.dropdown-icon');
            dropdowns.forEach((dropdown, index) => {
                const icon = dropdownIcons[index];
                dropdown.addEventListener('mouseenter', function() {
                    icon.style.transform = 'rotate(180deg) scale(1.2)';
                    icon.style.transition = 'transform 0.3s ease';
                });
                
                dropdown.addEventListener('mouseleave', function() {
                    icon.style.transform = 'rotate(0deg) scale(1)';
                });
            });
            
            // Smooth scroll for navigation
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    const target = document.querySelector(this.getAttribute('href'));
                    if (target) {
                        target.scrollIntoView({
                            behavior: 'smooth',
                            block: 'start'
                        });
                    }
                });
            });
            
            // Add scroll effect to navbar
            let lastScroll = 0;
            const navbar = document.querySelector('.top-bar');
            
            window.addEventListener('scroll', () => {
                const currentScroll = window.pageYOffset;
                
                if (currentScroll <= 0) {
                    navbar.classList.remove('scroll-up');
                    return;
                }
                
                if (currentScroll > lastScroll && !navbar.classList.contains('scroll-down')) {
                    navbar.classList.remove('scroll-up');
                    navbar.classList.add('scroll-down');
                } else if (currentScroll < lastScroll && navbar.classList.contains('scroll-down')) {
                    navbar.classList.remove('scroll-down');
                    navbar.classList.add('scroll-up');
                }
                lastScroll = currentScroll;
            });
            
            // Add keyboard navigation
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape') {
                    dropdowns.forEach(dropdown => {
                        const dropdownContent = dropdown.querySelector('.dropdown-content');
                        dropdownContent.style.display = 'none';
                        dropdownContent.style.opacity = '0';
                        dropdownContent.style.visibility = 'hidden';
                    });
                }
            });
            
            // Add touch support for mobile devices
            if ('ontouchstart' in window) {
                dropdowns.forEach(dropdown => {
                    dropdown.addEventListener('touchstart', function(e) {
                        e.preventDefault();
                        const dropdownContent = dropdown.querySelector('.dropdown-content');
                        const isVisible = dropdownContent.style.display === 'block';
                        
                        // Close all other dropdowns
                        dropdowns.forEach(other => {
                            if (other !== dropdown) {
                                const otherContent = other.querySelector('.dropdown-content');
                                otherContent.style.display = 'none';
                                otherContent.style.opacity = '0';
                                otherContent.style.visibility = 'hidden';
                            }
                        });
                        
                        if (!isVisible) {
                            dropdownContent.style.display = 'block';
                            setTimeout(() => {
                                dropdownContent.style.opacity = '1';
                                dropdownContent.style.visibility = 'visible';
                            }, 10);
                        } else {
                            dropdownContent.style.opacity = '0';
                            dropdownContent.style.visibility = 'hidden';
                            setTimeout(() => {
                                dropdownContent.style.display = 'none';
                            }, 400);
                        }
                    });
                });
            }
        });
    </script>

<script>
    // Number Animation Function
    function animateNumbers() {
        const statNumbers = document.querySelectorAll('.impact-stats .stat-number');
        
        statNumbers.forEach(stat => {
            const target = parseInt(stat.getAttribute('data-target'));
            if (isNaN(target)) return; // Skip if target is not a valid number
            
            const duration = 2000; // 2 seconds
            const increment = target / (duration / 16); // 60fps
            let current = 0;
            
            const updateNumber = () => {
                current += increment;
                if (current < target) {
                    stat.textContent = Math.floor(current).toLocaleString();
                    requestAnimationFrame(updateNumber);
                } else {
                    stat.textContent = target.toLocaleString();
                }
            };
            
            // Start animation when element is in viewport
            const observer = new IntersectionObserver((entries) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        updateNumber();
                        observer.unobserve(entry.target);
                    }
                });
            });
            
            observer.observe(stat);
        });
    }
    
    // Initialize animations when page loads
    document.addEventListener('DOMContentLoaded', animateNumbers);
</script>

